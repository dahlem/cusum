@article{citeulike:770860,
    abstract = {{Our whimsical title reflects our dismay over the rampant confusion regarding the use of key concepts from thermodynamics and information theory in various disciplines, but especially in relation to theories of biological evolution. After a brief introduction to this challenging literature, we begin by drawing a critically important distinction between ?order? and the informed ?functional organization? that characterizes living systems. We then outline what we believe is the appropriate paradigm for theorizing about the role of energy and information in biological processes; in essence, our paradigm is cybernetic. This is followed by a brief discussion of thermodynamics, with particular reference to its application to living systems. Two concepts that are well developed in the engineering literature but not commonly used elsewhere provide an approach that we believe is both more rigorous and more readily understood, namely the ?control volume? frame of reference and the concept of ?available energy?. Both of these concepts are defined in precise mathematical terms. We also critique some of the misuses of thermodynamic concepts. In Part II, we will discuss what we call the ?thermoeconomics? of living systems - that is, a cybernetic and economic approach to analyzing the role of available energy in biological evolution - and we will relate this paradigm to a distinction that we draw between various statistical and/or structural definitions of information and what we call ?control information?. We will critique information theory and we will define control information in cybernetic terms not as a ?thing? but as an attribute of the relationships between things - namely, the capacity (know-how) to control the acquisition, disposition and utilization of matter/energy in purposive (teleonomic) processes. We will also suggest how control information can be measured empirically, and we will propose a methodology for linking thermodynamics and information theory that contrasts sharply with existing approaches to this problem. Finally, we will argue that in living systems thermodynamic processes may be subject to certain law-like ?bioeconomic? principles. We will also elucidate some implications. {\copyright} 1998 John Wiley \& Sons, Ltd.}},
    address = {Institute for the Study of Complex Systems, Palo Alto, CA, USA; Woodard Professor of Science, Technology and Society, and of Mechanical Engineering, Emeritus, Stanford University, Stanford, CA, USA},
    author = {Corning, Peter A. and Kline, Stephen J.},
    citeulike-article-id = {770860},
    citeulike-linkout-0 = {http://dx.doi.org/10.1002/(sici)1099-1743(199807/08)15:4%3C273::aid-sres200%3E3.0.co;2-b},
    citeulike-linkout-1 = {http://www3.interscience.wiley.com/cgi-bin/abstract/71007246/ABSTRACT},
    comment = {(private-note)This article gives some detailed insights into the philosophy of science with respect to a systems theory of using thermoeconomics. Also, it attempts to explain the different aspects of entropy in the scientific literature and concludes with his own understanding of thermodynamics and entropy understanding of systems science.
Interesting to remember is his cybernetic approach to systems science. From the article it appears that this is the right framework to follow in my own research. Herman Haken is one of the leading cyberneticists and it would be necessary to read the references to his articles.
Also, the distinction of self-organisation and self-ordering is important, because Ilya Prigogine's Bernard cells might fall into the self-ordering category. Again, it would be important to understand the distinction between both and the criticism Corning lends to existing work (past and recent work).},
    doi = {10.1002/(sici)1099-1743(199807/08)15:4%3C273::aid-sres200%3E3.0.co;2-b},
    journal = {Systems Research and Behavioral Science},
    keywords = {1998, complex\_system, entropy, philosophy, science, self-organization, systems, thermodynamics},
    number = {4},
    pages = {273--295},
    posted-at = {2006-07-24 09:18:16},
    priority = {0},
    title = {{Thermodynamics, information and life revisited, Part I: "To be or entropy"}},
    url = {http://dx.doi.org/10.1002/(sici)1099-1743(199807/08)15:4%3C273::aid-sres200%3E3.0.co;2-b},
    volume = {15},
    year = {1998}
}

