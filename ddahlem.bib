    doi = {10.1103/physreve.82.016105},
    journal = {Physical Review E},
    keywords = {2010, complex, diffusion, information, network, social, weak-ties},
    month = jul,
    number = {1},
    pages = {016105+},
    posted-at = {2010-08-19 21:44:20},
    priority = {0},
    publisher = {American Physical Society},
    title = {{Weak ties: Subtle role of information diffusion in online social networks}},
