    journal = {High Performance Distributed Computing, 2003. Proceedings. 12th IEEE International Symposium on},
    keywords = {2003, failure, grid, reliability, workflow},
    month = jun,
    pages = {126--137},
    posted-at = {2006-09-09 17:08:17},
