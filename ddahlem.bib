    pages = {101--133},
    posted-at = {2006-09-19 12:42:07},
    priority = {0},
    publisher = {Institute of Mathematical Statistics},
    title = {{Interval estimation for a binomial proportion}},
    url = {http://www.ams.org/mathscinet-getitem?mr=1861069},
