    citeulike-linkout-3 = {http://www3.interscience.wiley.com/cgi-bin/abstract/118808484/ABSTRACT},
    doi = {10.1111/j.1467-9868.2004.05304.x},
    issn = {1369-7412},
    journal = {Journal of the Royal Statistical Society: Series B (Statistical Methodology)},
    keywords = {2004, analysis, bayesian, inference, model, sensitivity, simulation, uncertainty},
    month = aug,
    number = {3},
    pages = {751--769},
    posted-at = {2008-02-11 10:39:04},
