    journal = {Ecological Modelling},
    keywords = {2003, bayesian, bmc, inference, mcmc, monte-carlo},
    month = jan,
    number = {2-3},
    pages = {269--277},
    posted-at = {2007-10-03 08:27:23},
    priority = {0},
    title = {{On Monte Carlo methods for Bayesian inference}},
