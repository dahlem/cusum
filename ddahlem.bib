    issn = {1533-7928},
    journal = {J. Mach. Learn. Res.},
    keywords = {2003, function-approximation, least-squares-policy-iteration, linear, reinforcement-learning},
    pages = {1107--1149},
    posted-at = {2009-06-29 11:55:37},
    priority = {0},
    publisher = {MIT Press},
    title = {{Least-squares policy iteration}},
    url = {http://portal.acm.org/citation.cfm?id=945365.964290},
    volume = {4},
