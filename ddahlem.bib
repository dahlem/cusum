    keywords = {2002, distributed, load-balancing, multi-agent, task-assignment},
    month = nov,
    number = {8},
    pages = {787--801},
    posted-at = {2009-10-17 16:10:35},
    priority = {0},
    title = {{On load balancing for distributed multiagent computing}},
