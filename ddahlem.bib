    citeulike-linkout-1 = {http://projecteuclid.org/Dienst/UI/1.0/Summarize/euclid.cms/1119655353},
    comment = {(private-note)This paper outlines an equation-free framework for multiscale modeling. It may be an important addition to multiscale modeling of re-enforcement learning for dynamic service selection.},
    journal = {Communications in Mathematical Sciences},
    keywords = {2003, equation-free, gap-tooth, multiscale, patch-dynamics, simulation},
    number = {4},
    pages = {715--762},
    posted-at = {2007-03-19 18:31:55},
    priority = {0},
    publisher = {International Press of Boston},
    title = {{Equation-Free, Coarse-Grained Multiscale Computation: Enabling Mocroscopic Simulators to Perform System-Level Analysis}},
