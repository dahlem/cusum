@inproceedings{citeulike:790487,
    abstract = {{Semantic Web Services are a promising combination of Semantic Web and Web service technology, aiming at providing means of automatically executing, discovering and composing semantically marked-up Web services. We envision peer-to-peer networks which allow for carrying out searches in real-time on permanently reconfiguring networks to be an ideal infrastructure for deploying a network of Semantic Web Service providers. However, P2P networks evolving in an unorganized manner suffer from serious scalability problems, limiting the number of nodes in the network, creating network overload and pushing search times to unacceptable limits. We address these problems by imposing a deterministic shape on P2P networks: We propose a graph topology which allows for very efficient broadcast and search, and we provide an efficient topology construction and maintenance algorithm which, crucial to symmetric peer-to-peer networks, does neither require a central server nor super nodes in the network. We show how our scheme can be made even more efficient by using a globally known ontology to determine the organization of peers in the graph topology, allowing for efficient concept-based search.}},
    author = {Schlosser, M. and Sintek, M. and Decker, S. and Nejdl, W.},
    booktitle = {Proceedings of the Second International Conference on Peer-to-Peer Computing, 2002. (P2P 2002)},
    citeulike-article-id = {790487},
    citeulike-linkout-0 = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=1046319},
    comment = {(private-note)The authors describe an unstructured P2P topology based on hypercubes that partitions the space into globally known ontological concepts. Therefore no central server or super-peers are required. However, the assumption is on some globally known ontologies. This assumption cannot be supported in an internet SOA.},
    journal = {Peer-to-Peer Computing, 2002. (P2P 2002). Proceedings. Second International Conference on},
    keywords = {2002, ontology, p2p, registry, web-service},
    pages = {104--111},
    posted-at = {2006-08-09 11:12:09},
    priority = {0},
    title = {{A scalable and ontology-based P2P infrastructure for Semantic Web Services}},
    url = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=1046319},
    year = {2002}
}

@article{citeulike:790478,
    abstract = {{Web Services are emerging as a dominant paradigm for constructing and composing distributed business applications and enabling enterprise-wide interoperability. A critical factor to the overall utility of Web Services is a scalable, flexible and robust discovery mechanism. This paper presents a Peer-to-Peer (P2P) indexing system and associated P2P storage that supports large-scale, decentralized, real-time search capabilities. The presented system supports complex queries containing partial keywords and wildcards. Furthermore, it guarantees that all existing data elements matching a query will be found with bounded costs in terms of number of messages and number of nodes involved. The key innovation is a dimension reducing indexing scheme that effectively maps the multidimensional information space to physical peers. The design and an experimental evaluation of the system are presented.}},
    address = {Hingham, MA, USA},
    author = {Schmidt, Cristina and Parashar, Manish},
    citeulike-article-id = {790478},
    citeulike-linkout-0 = {http://portal.acm.org/citation.cfm?id=971563},
    citeulike-linkout-1 = {http://dx.doi.org/10.1023/b:wwwj.0000017210.55153.3d},
    comment = {(private-note)This paper approaches decentralised registries from a different angle than our work. It uses a keyword indexing and clustering mechanism (Space-Filling-Curves) in order to allow search queries that are not complete (i.e., partial keywords, wildcards, etc). Consequently, DHTs that are based on a unique identifier space (can, pastry, tapestry) are not applicable. In contrast, our approach partitions the registries into yellow pages (registries over the GT) and identifier/location mapping (DHT). This allows us to take into account the load differences on these topologies: the GT is targeted towards low load (registry endpoints can be cached), whereas the DHT gives high load baring guarantees.},
    doi = {10.1023/b:wwwj.0000017210.55153.3d},
    issn = {1386-145X},
    journal = {World Wide Web},
    keywords = {2004, discovery, p2p, registry, web-service},
    month = jun,
    number = {2},
    pages = {211--229},
    posted-at = {2006-08-09 10:52:33},
    priority = {0},
    publisher = {Kluwer Academic Publishers},
    title = {{A Peer-to-Peer Approach to Web Service Discovery}},
    url = {http://dx.doi.org/10.1023/b:wwwj.0000017210.55153.3d},
    volume = {7},
    year = {2004}
}

@article{citeulike:790057,
    abstract = {{Certainly, one of the most common goals in applied research is comparing two or more groups in terms of some measure of location, that is, a quantity intended to represent the typical subject or object under study. Of course, the measure of location routinely used is the population mean. If there is no difference between the distributions associated with two or more groups, standard methods for comparing means appear to provide good control over the probability of a Type 1 error. In this article, the problem that arises in using conventional statistical methods to compare group means and then discuss some solutions is reviewed. Standard nonparametric methods do not correct the problem, nor do some of the better known improvements for comparing means. There are, however, new methods that can help applied researchers.}},
