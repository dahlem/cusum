    month = feb,
    posted-at = {2006-08-20 10:10:56},
    priority = {3},
    title = {{Information Theory - The Bridge Connecting Bounded Rational Game Theory and Statistical Physics}},
    url = {http://arxiv.org/abs/cond-mat/0402508},
    year = {2004}
}

